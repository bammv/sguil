        -c { set state conf }
        -d { set state debug_level }
        default { DisplayUsage $argv0 }
      }
    }
    conf { set CONF_FILE $arg; set state flag }
    debug_level { set DEBUG_OVERRIDE 1; set DEBUG $arg; set DEBUG_LEVEL $arg; set state flag }
    default { DisplayUsage $argv0 }
  }
}

